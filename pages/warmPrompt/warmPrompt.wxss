/* pages/warmPrompt/warmPrompt.wxss */
.main {
  background-color: #ffffff;
  padding: 40rpx 64rpx 0;
  overflow: auto;
}
.promptBox{
  margin-bottom:64rpx ;
}
.promptTitle{
  display: flex;
}
.numBox {
  width: 48rpx;
  height: 48rpx;
  background: #12A86D;
  border-radius: 50%;
  color: #ffffff;
  text-align: center;
  line-height: 48rpx;
  margin-right: 10rpx;
}
.ItemTitle{
  flex: 1;
  flex-wrap: wrap;
  line-height: 48rpx;
}
.ItemTitle text.bold{
  font-weight: bold;
}
.promptBox image{
  width: 100%;
  /* height: 160rpx; */
  margin-top: 8rpx;
}

button:not([size=mini]).nextStep {
  width: 80%;
  border-radius: 20px;
  margin: 80rpx 0 40rpx;
  margin-left: 10%;
}
