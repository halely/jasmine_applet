.serviceAreaItem .direction {
  font-size: 24rpx;
  color: #666666;
  line-height: 36rpx;
  margin-top: 20rpx;
}

.serviceAreaItem .item_head .serviceAreaInfo .serviceAreaInfo_head text {
  font-size: 13px;
  color: #666666;
  line-height: 18px;
}


.serviceAreaInfo_main_item image {
  width: 14px;
  height: 14px;
  margin-right: 10rpx;
  flex-shrink: 0;
}

.serviceAreaItem .parkingInfo {
  flex: 1;
}

.serviceAreaItem .parkingInfo .parkingInfo_item {
  padding: 2px 4px;
  border-radius: 8px;
  background-color: #F7F7F7;
  margin: 3px 6px 3px 0;
  flex-wrap: wrap;
  font-size: 12px;
  color: #666666;
  line-height: 17px;
  align-items: center;
  display: inline-flex;
}

.serviceAreaItem .parkingInfo_item>text {
  font-size: 14px;
  font-family: DINAlternate-Bold, DINAlternate;
  font-weight: bold;
  color: #333333;
  line-height: 19px;
  margin-left: 2px;
}

.serviceAreaItem .item_foot {
  display: flex;
  margin-top: 10px;
}

.serviceAreaItem .perCapita {
  background: rgba(7, 193, 96, 0.08);
  border-radius: 8rpx;
  color: #07C160;
  font-size: 22rpx;
  line-height: 18px;
  height: 18px;
  width: 52px;
  display: inline-block;
  text-align: center;
  margin-right: 14px;
  flex-shrink: 0;
}

.serviceAreaItem .perCapita.error {
  color: #FF2020;
  background: #fcf0f0;
}

.serviceAreaItem .recommendBox {
  flex: 1;
  overflow: hidden;
}

.serviceAreaItem .recommendBox>text {
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  position: relative;
}


.serviceAreaItem .sales_status {
  width: 100%;
  display: flex;
  font-size: 12px;
  color: #666666;
  line-height: 17px;
  margin-top: 8px;
}

.serviceAreaItem .sales_status text {
  margin: 0 30px 0 8px;
}

.serviceAreaInfo_main_item image {
  width: 14px;
  height: 14px;
  margin-right: 10rpx;
  flex-shrink: 0;
}

.parkingInfo {
  flex: 1;
}

.parkingInfo .parkingInfo_item {
  padding: 2px 4px;
  border-radius: 8px;
  background-color: #F7F7F7;
  margin: 3px 6px 3px 0;
  flex-wrap: wrap;
  font-size: 12px;
  font-weight: 400;
  color: #666666;
  line-height: 17px;
  align-items: center;
  display: inline-flex;
}

.parkingInfo_item>text {
  font-size: 14px;
  font-family: DINAlternate-Bold, DINAlternate;
  font-weight: bold;
  color: #333333;
  line-height: 19px;
  margin-left: 2px;
}

.cheerItem {
  border-radius: 4px;
  border: 1px solid #979797;
  color: #979797;
  font-size: 12px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 17px;
  display: inline-block;
  padding: 0 4px;
  margin: 3px 4px 3px 0;
}

.cheerItem.diesel {
  border-radius: 8px;
}

.item_foot {
  display: flex;
  margin-top: 10px;
}

.recommendBox {
  flex: 1;
  overflow: hidden;
}

.recommendBox>text {
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.sales_status {
  width: 100%;
  display: flex;
  font-size: 12px;
  color: #666666;
  line-height: 17px;
  margin-top: 8px;
}

.sales_status text {
  margin: 0 30px 0 8px;
}

.no_location {
  width: 100%;
  height: 300rpx;
  display: flex;
  font-size: 14px;
  color: #999999;
  line-height: 20px;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.no_location text {
  font-weight: 500;
  color: #07C160;
  line-height: 20px;
  background-color: rgba(7, 193, 96, 0.07);
  padding: 4rpx 16rpx;
  margin-top: 10rpx;
}