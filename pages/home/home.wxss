.head {
  align-items: center;
  padding: 0 20px;
  box-sizing: border-box;
  font-weight: bold;
}

.entranceBox {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  background-color: #FFFFFF;
  margin-top: 20rpx;
}

.entranceBox .entranceItem {
  width: 50%;
  padding: 66rpx 52rpx;
  box-sizing: border-box;
  display: inline-flex;
  justify-content: space-between;
  border: 2rpx solid #f5f5f5;
}

.entranceBox .entranceItem .entranceTitle {
  display: flex;
  flex-direction: column;
  font-size: 12px;
  color: #999999;
  line-height: 17px;
}

.entranceBox .entranceItem .entranceTitle>text:first-child {
  font-size: 16px;
  font-weight: 600;
  color: #333333;
  line-height: 22px;
  margin-bottom: 8rpx;
}

.entranceBox .entranceItem .entranceTitle .carType>text {
  display: inline-block;
  font-size:20rpx;
  font-weight: 600;
  color: #0089FF;
  line-height: 18px;
  background-color: rgba(0, 137, 255, 0.1);
  padding: 4rpx 10rpx;
  border-radius: 16rpx;
  margin: 10rpx 0 0 10rpx;
}
.entranceBox .entranceItem .entranceTitle .carType>text:first-child{
  color: #07C160;
  background-color: rgba(7, 193, 96, 0.1);
  margin-left: 0;
}
.entranceBox .entranceItem image {
  width: 88rpx;
  height: 88rpx;
}

.tab_box {
  display: flex;
  background-color: #FFFFFF;
  margin-top: 20rpx;
  padding: 14px 21px;
  flex-direction: column;
}

.tab_box .tabHead {
  width: 100%;
}

.tab_box .tabHead .tab_item {
  font-size: 14px;
  float: left;
  color: #333333;
  margin-right: 40rpx;
  line-height: 20px;
}

.tab_box .tabHead .tab_item.selected {
  color: #07C160;
  font-size: 16px;
  font-weight: 600;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tab_box .tabHead .tab_item.selected::after {
  content: '';
  width: 30px;
  height: 3px;
  background-color: #07C160;
  margin-top: 8rpx;
}

.tab_box .tabHead .moreBut {
  float: right;
}

.tab_box .tabMain {
  margin-top: 20rpx;
}

.main {
  overflow-y: auto;
}


.swiper_box {
  width: 100%;
  padding: 20px 0 20px 20px;
  box-sizing: border-box;
}

.tollstationItem,
.serviceAreaItem {
  width: 100%;
  background-color: #FFFFFF;
  padding: 20px 14px;
  box-sizing: border-box;
  box-shadow: 0px 0px 6px 0px rgba(0, 0, 0, 0.1);
  border-radius: 4px;

}

.tollstation_name {
  font-size: 18px;
  font-weight: 500;
  color: #333333;
  line-height: 25px;
  display: flex;
  align-items: center;

}

.tollstationItem .item_head {
  display: flex;
}

.tollstationItem .item_head .tollStationImg {
  width: 52px;
  height: 52px;
  margin-right: 24rpx;
}


.distance {
  font-size: 13px;
  font-weight: 400;
  color: #666666;
  line-height: 18px;
  margin-top: 11px;
  display: block;
}

.item_main {
  margin-top: 10px;
}

.tollstation_state {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 6px 0;
}

.tollstation_state .state_title {
  flex: 1;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.stateInfo {
  display: flex;
  justify-content: space-between;
  width: 160px;
}

.stateInfo text {
  display: block;
  padding: 4px 8px;
  background-color: #F4F4F4;
  font-size: 14px;
  font-weight: 500;
  color: #07C160;
  line-height: 20px;
}

.stateInfo text.close {
  color: #FF2020;
}

.serviceAreaItem .item_head {
  display: flex;
}

.serviceAreaItem {
  width: 100%;
  padding: 40rpx 32rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  margin-bottom: 20rpx;
}

.serviceAreaItem .item_head {
  display: flex;
}

.serviceAreaItem .item_head .serviceAreaImg {
  width: 52px;
  height: 52px;
  margin-right: 14px;
}

.serviceAreaItem .item_head .serviceAreaInfo {
  flex: 1;
}

.serviceAreaItem .item_head .serviceAreaInfo .serviceAreaInfo_head {
  font-size: 18px;
  font-weight: bold;
  color: #333333;
  line-height: 25px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

