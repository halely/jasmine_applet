/* pages/home/home.wxss */
.container .Home_nav {
  height: 294px;
  display: flex;
  flex-direction: column;
  font-size: 14px;
  font-weight: 400;
  color: rgba(255, 255, 255, 0.8);
  line-height: 20px;
  position: relative;
  justify-content: space-between;
}

.Home_nav_bg {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.Home_nav .placeBox {
  position: relative;
  z-index: 2;
  margin-left: 16px;
  display: flex;
  align-items: center;
}

.Home_nav .evaluate {
  position: relative;
  z-index: 2;
  align-self: center;
  margin-bottom: 28rpx;
  background: #FFFFFF;
  border-radius: 10px;
  font-weight: 500;
  color: #1F7F69 !important;
  line-height: 14px;
  font-size: 10px;
  padding: 4rpx 16rpx;
}

.placeBox .positionImg {
  width: 16px;
  height: 16px;
  margin-right: 8rpx;
}

